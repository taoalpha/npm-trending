    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^13.1.0",
    "@types/request-promise": "^4.1.42",
    "gh-pages": "2.0",
    "handlebars": "^4.0.5",
    "sass": "^1.14.1",
    "ts-node": "^8.4.0",
    "typescript": "^3.0.3"
