  "main": "index.js",
  "scripts": {
    "fetch": "ts-node index.ts",
    "compile": "mkdir -p dist && cp -rf template/* dist/ && sass dist/lib/main.scss dist/lib/main.css && tsc --out dist/lib/main.js dist/lib/main.ts --lib 'dom, es2017' --skipLibCheck --target ES5 --module amd",
